{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/manifest.json",
      "dest": "/api/index.js"
    },
    {
      "src": "/catalog/:type/:id.json",
      "dest": "/api/index.js"
    },
    {
      "src": "/meta/:type/:id.json",
      "dest": "/api/index.js"
    },
    {
      "src": "/stream/:type/:id.json",
      "dest": "/api/index.js"
    },
    {
      "src": "/configure",
      "dest": "/api/index.js"
    },
    {
      "src": "/generate-url",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ]
}
