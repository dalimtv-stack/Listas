{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/:configId/manifest.json",
      "dest": "/api/index.js"
    },
    {
      "src": "/:configId/catalog/:type/:id(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/:configId/catalog/:type/:id.json",
      "dest": "/api/index.js"
    },
    {
      "src": "/:configId/meta/:type/:id(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/:configId/meta/:type/:id.json",
      "dest": "/api/index.js"
    },
    {
      "src": "/:configId/stream/:type/:id(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/:configId/stream/:type/:id.json",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ]
}
