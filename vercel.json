{
  "version": 2,
  "builds": [
    { "src": "api/index.js", "use": "@vercel/node" },
    { "src": "api/cleanup.js", "use": "@vercel/node" },
    { "src": "api/poster-con-hora.js", "use": "@vercel/node" },
    { "src": "api/cleanupposters.js", "use": "@vercel/node" },
    { "src": "api/regenerate-posters.js", "use": "@vercel/node" },
    { "src": "api/config-index.js", "use": "@vercel/node" },
    { "src": "api/check_playlist.js", "use": "@vercel/node" },
    { "src": "api/resolution.js", "use": "@vercel/node" },
    { "src": "api/m3u_editor.js", "use": "@vercel/node" },
    { "src": "api/epg_precarga.js", "use": "@vercel/node" },
    { "src": "api/upload-image.js", "use": "@vercel/node" }
  ],
  "crons": [
    { "path": "/api/regenerate-posters", "schedule": "00 4 * * *" },
    { "path": "/api/epg_precarga", "schedule": "00 6 * * *" }
  ],
  "routes": [
    { "src": "/manifest.json", "dest": "/api/index.js" },
    { "src": "/([0-9a-f-]+)/manifest.json", "dest": "/api/index.js" },
    { "src": "/([0-9a-f-]+)/catalog/:type/:id.json", "dest": "/api/index.js" },
    { "src": "/catalog/:type/:id.json", "dest": "/api/index.js" },
    { "src": "/([0-9a-f-]+)/meta/:type/:id.json", "dest": "/api/index.js" },
    { "src": "/meta/:type/:id.json", "dest": "/api/index.js" },
    { "src": "/([0-9a-f-]+)/stream/:type/:id.json", "dest": "/api/index.js" },
    { "src": "/stream/:type/:id.json", "dest": "/api/index.js" },
    { "src": "/Acceso", "dest": "/api/config-index.js" },
    { "src": "/configure", "dest": "/api/index.js" },
    { "src": "/Resolution", "dest": "/api/resolution.js" },
    { "src": "/upload-image", "dest": "/api/upload-image.js" },
    { "src": "/generate-url", "dest": "/api/index.js" },
    { "src": "/debug-kv/([0-9a-f-]+)", "dest": "/api/index.js" },

    { "src": "/cleanup", "dest": "/api/cleanup.js" },
    { "src": "/poster-con-hora", "dest": "/api/poster-con-hora.js" },
    { "src": "/cleanupposters", "dest": "/api/cleanupposters.js" },
    { "src": "/comprobar", "dest": "/api/check_playlist.js" },
    { "src": "/editor", "dest": "/api/m3u_editor.js" },
    { "src": "/editor/data", "dest": "/api/m3u_editor.js" },
    { "src": "/editor/data", "methods": ["POST"], "dest": "/api/m3u_editor.js" },
    { "src": "/epg_precarga", "dest": "/api/epg_precarga.js" },
    { "src": "/regenerate-posters", "dest": "/api/regenerate-posters.js" },

    { "src": "/(.*)", "dest": "/api/index.js" }
  ]
}
